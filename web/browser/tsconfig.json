{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "dom",
      "es2016"
    ],
    "outDir": "../build/browser",
    "sourceMap": true,
    "jsx": "react",
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
